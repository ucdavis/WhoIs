@model IEnumerable<WhoIs.Models.User>

@{
    ViewBag.Title = "Search";
}

<h2>Search</h2>

<table>
    <tr>
        <th>
            Full Name
        </th>
        <th>
            Email
        </th>
        <th>
            LoginId
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FullName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LoginId)
        </td>
    </tr>
}

</table>
